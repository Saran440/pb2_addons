		<![CDATA[select rp.*, rpc.name as category_name, rct.name as township_name, rcd.name as district_name, rcp.name as province_name, sum(l.credit - l.debit) as balance
from account_move_line l
left join res_partner rp on l.partner_id = rp.id
left join res_partner_category rpc on rp.category_id = rpc.id
left join res_country_township rct on rp.township_id = rct.id
left join res_country_district rcd on rp.district_id = rcd.id
left join res_country_province rcp on rp.province_id = rcp.id
