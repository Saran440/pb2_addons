			<field name="header">False</field>
			<field name="jasper_report">True</field>
			<field name="jasper_output">xls</field>
			<field name="copies">1</field>
			<field name="force_locale">th_TH</field>
			<field name="download_filename"></field>
		</record>

		<record id="customer_payment_history_values" model="ir.values">
			<field name="name">Customer Payment History</field>
			<field name="model">payment.history.view</field>
			<field name="value_unpickle" eval="'ir.actions.report.xml, ' +str(customer_payment_history)"/>
