<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		# Payable Confirmation Letter
		<record id="payable_confirmation_letter" model="ir.actions.report.xml">
			<field name="auto">True</field>
			<field name="model">account.move.line</field>
			<field name="name">Payable Confirmation Letter</field>
			<field name="report_name">payable_confirmation_letter</field>
			<field name="report_rml">pabi_account_report/reports/jasper_report_payable_confirmation_letter.jrxml</field>
			<field name="menu">True</field>
			<field name="header">False</field>
			<field name="jasper_report">True</field>
			<field name="jasper_output">pdf</field>
			<field name="copies">1</field>
			<field name="force_locale">th_TH</field>
			<field name="download_filename"></field>
		</record>

		<record id="payable_confirmation_letter_values" model="ir.values">
			<field name="name">Payable Confirmation Letter</field>
			<field name="model">account.move.line</field>
			<field name="value_unpickle" eval="'ir.actions.report.xml, ' +str(payable_confirmation_letter)"/>
		</record>

