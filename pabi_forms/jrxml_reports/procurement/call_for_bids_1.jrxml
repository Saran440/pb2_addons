	<queryString>
		<![CDATA[SELECT pd.id as pd_id,
pr.id as pr_id,
(SELECT code FROM operating_unit
WHERE id = pd.operating_unit_id LIMIT 1) as ou_name,
(SELECT name FROM operating_unit
WHERE id = pd.operating_unit_id LIMIT 1) as ou_key,
pd.name as pd_number,
pr.name as pr_sap,
CONCAT(
COALESCE((SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT rpt.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
LEFT JOIN res_partner_title rpt
ON rpt.id = he.title_id
WHERE ru.id = pd.request_uid) AND it.name LIKE 'res.partner.title,name') || ' ', ''),
(SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT he.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
WHERE ru.id = pd.request_uid) AND it.name LIKE 'hr.employee,first_name'),
' ',
(SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT he.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
WHERE ru.id = pd.request_uid) AND it.name LIKE 'hr.employee,last_name')
) as requested_by,
(SELECT hp.description
FROM hr_employee hr
LEFT JOIN hr_position hp
ON hp.id = hr.position_id
where hr.employee_code =
(SELECT login from res_users where id = pr.requested_by  LIMIT 1)
 LIMIT 1) as position,
(
SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT he.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
LEFT JOIN res_section rs
ON  rs.id = he.section_id
WHERE ru.id = pr.requested_by  LIMIT 1) AND it.name LIKE 'res.section,name' LIMIT 1
) as department,
pcht.name as purchase_type_name,
pd.description as objective,
pr.amount_total,
pr.delivery_address as deliver_to,
pm.name as method,
ppr.name as price_range,
pd.doc_body as footer,
po.name as rfq_number,
(SELECT rp.name FROM res_partner rp
WHERE rp.id = po.partner_id  LIMIT 1) as rfq_supplier,
(SELECT rp.taxbranch FROM res_partner rp
WHERE rp.id = po.partner_id  LIMIT 1) as rfq_taxbranch,
po.amount_total as rfq_amount_total,
po.minimum_planned_date-DATE(now()) as minimum_planned_date,
pd.delivery_detail as delivery_detail,
pd.standard_price as standard_price,
pcon.name as purchase_condition,
pcondet.name as purchase_condition_detail,
CONCAT(
COALESCE((SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT rpt.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
LEFT JOIN res_partner_title rpt
ON rpt.id = he.title_id
WHERE ru.id = pd.user_id) AND it.name LIKE 'res.partner.title,name') || ' ', ''),
(SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT he.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
WHERE ru.id = pd.user_id) AND it.name LIKE 'hr.employee,first_name'),
' ',
(SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT he.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
WHERE ru.id = pd.user_id) AND it.name LIKE 'hr.employee,last_name')
) as full_responsible_uid,
CONCAT(
(SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT he.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
WHERE ru.id = pd.user_id) AND it.name LIKE 'hr.employee,first_name'),
' ',
(SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT he.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
WHERE ru.id = pd.user_id) AND it.name LIKE 'hr.employee,last_name')
) as responsible_uid,
CONCAT(
COALESCE((SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT rpt.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
LEFT JOIN res_partner_title rpt
ON rpt.id = he.title_id
WHERE ru.id = pd.verify_uid) AND it.name LIKE 'res.partner.title,name') || ' ', ''),
(SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT he.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
WHERE ru.id = pd.verify_uid) AND it.name LIKE 'hr.employee,first_name'),
' ',
(SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT he.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
WHERE ru.id = pd.verify_uid) AND it.name LIKE 'hr.employee,last_name')
) as full_verified_uid,
CONCAT(
(SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT he.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
WHERE ru.id = pd.verify_uid) AND it.name LIKE 'hr.employee,first_name'),
' ',
(SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT he.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
WHERE ru.id = pd.verify_uid) AND it.name LIKE 'hr.employee,last_name')
) as verified_uid,
CONCAT(
(SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT he.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
WHERE ru.id = pd.user_id) AND it.name LIKE 'hr.employee,first_name'),
' ',
(SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT he.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
WHERE ru.id = pd.user_id) AND it.name LIKE 'hr.employee,last_name')
) as responsible_uid,
CONCAT(
COALESCE((SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT rpt.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
LEFT JOIN res_partner_title rpt
ON rpt.id = he.title_id
WHERE ru.id = pd.doc_approve_uid) AND it.name LIKE 'res.partner.title,name') || ' ', ''),
(SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT he.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
WHERE ru.id = pd.doc_approve_uid) AND it.name LIKE 'hr.employee,first_name'),
' ',
(SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT he.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
WHERE ru.id = pd.doc_approve_uid) AND it.name LIKE 'hr.employee,last_name')
) as full_approve_uid,
CONCAT(
(SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT he.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
WHERE ru.id = pd.doc_approve_uid) AND it.name LIKE 'hr.employee,first_name'),
' ',
(SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT he.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
WHERE ru.id = pd.doc_approve_uid) AND it.name LIKE 'hr.employee,last_name')
) as approve_uid,
pd.create_date as pd_create_date,
pd.date_verify as pd_verify_date,
pd.date_doc_approve as pd_approve_date,
po.amount_total_text_th
FROM purchase_requisition pd
LEFT JOIN operating_unit ou
ON ou.id = pd.operating_unit_id
LEFT JOIN purchase_request pr
ON pr.id = (SELECT DISTINCT prl.request_id FROM purchase_request_purchase_requisition_line_rel prpd
LEFT JOIN purchase_request_line prl
ON prl.id = prpd.purchase_request_line_id
LEFT JOIN purchase_requisition_line pdl
ON pdl.id = prpd.purchase_requisition_line_id
WHERE pdl.requisition_id = pd.id LIMIT 1)
LEFT JOIN purchase_method pm
ON pm.id = pd.purchase_method_id
LEFT JOIN purchase_price_range ppr
ON ppr.id = pd.purchase_price_range_id
LEFT JOIN purchase_type pcht
ON pcht.id = pd.purchase_type_id
LEFT JOIN purchase_condition pcon
ON pcon.id = pd.purchase_condition_id
LEFT JOIN purchase_condition_detail pcondet
ON pcondet.id = pd.purchase_condition_detail_id
LEFT JOIN purchase_order po
ON po.requisition_id = pd.id AND po.state = 'done'
WHERE $X{IN, pd.id, PIDS}]]>
	</queryString>
