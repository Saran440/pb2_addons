	<subDataset name="request_approval">
		<parameter name="ids" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[SELECT
CONCAT(
COALESCE((SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT rpt.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
LEFT JOIN res_partner_title rpt
ON rpt.id = he.title_id
WHERE ru.id = pr.requested_by) AND it.name LIKE 'res.partner.title,name') || ' ', ''),
(SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT he.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
WHERE ru.id = pr.requested_by) AND it.name LIKE 'hr.employee,first_name'),
' ',
(SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT he.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
WHERE ru.id = pr.requested_by) AND it.name LIKE 'hr.employee,last_name')
) as requested_by,
pr.create_date,
CONCAT(
COALESCE((SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT rpt.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
LEFT JOIN res_partner_title rpt
ON rpt.id = he.title_id
WHERE ru.id = pr.assigned_to) AND it.name LIKE 'res.partner.title,name') || ' ', ''),
(SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT he.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
WHERE ru.id = pr.assigned_to) AND it.name LIKE 'hr.employee,first_name'),
' ',
(SELECT value FROM ir_translation it
WHERE it.res_id = (SELECT he.id FROM res_users ru
LEFT JOIN hr_employee he
ON ru.login = he.employee_code
WHERE ru.id = pr.assigned_to) AND it.name LIKE 'hr.employee,last_name')
) as approver,
pr.date_approve
FROM purchase_request pr
WHERE pr.id = $P{ids}]]>
		</queryString>
		<field name="requested_by" class="java.lang.String">
			<fieldDescription><![CDATA[Requested by]]></fieldDescription>
		</field>
		<field name="create_date" class="java.sql.Timestamp">
			<fieldDescription><![CDATA[Created on]]></fieldDescription>
