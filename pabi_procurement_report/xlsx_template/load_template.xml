<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <!-- This function is called after all ir.attachment is loaded
            to load the file content to relevant template_ids -->
        <function model="ir.attachment" name="load_xlsx_template">
            <value eval="[
                ref('xlsx_report_pabi_purchase_summarize'),
                ref('xlsx_report_pabi_standard_asset'),
                ref('xlsx_report_pabi_stock_card_for_accounting'),
                ref('xlsx_report_pabi_monthly_work_acceptance'),
                ref('xlsx_report_pabi_sla_balance'),
                ref('xlsx_report_pabi_supplier_list'),
                ref('xlsx_report_pabi_stock_balance'),
                ref('xlsx_report_pabi_supplier_evaluation'),
                ref('xlsx_report_pabi_comparison_pr_po'),
            ]"/>
            <value>pabi_procurement_report/xlsx_template</value>
        </function>
    </data>
</openerp>
